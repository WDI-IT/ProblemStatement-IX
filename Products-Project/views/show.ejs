<%- include("partials/header"); -%>

<div class="content">
  <h1>All Product Details</h1>
<%  products.forEach(function(product){%>
    <h5>Product ID : <%=product.id%></h5>
    <h5>Product Name : <%=product.name%></h5>
    <h5>Product Image :</h5><img class="img3" src="<%=product.link%>" alt="product-image can't download.">
    <h5>Product Description: <%=product.desc%></h5>
    <hr>
  <%});%>
</div>
<%- include("partials/footer"); -%>
